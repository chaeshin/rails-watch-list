<div class="container m-5">
  <h1><%= @list.name %></h1>
    <div>
      <% @list.bookmarks.each do |bookmark| %>
        <div class='d-flex'>
          <%= render 'card', bookmark: bookmark, movie: bookmark.movie %>
        </div>
      <% end %>
    </div>
</div>
<%= link_to 'add a movie', new_list_bookmark_path(@list), class: 'm-5 p-2 border rounded-pill shadow-sm text-decoration-none btn btn-light align-item-center' %>
</div>
